<%- model_class = @diagnosed_object.class -%>
<%= provide(:title, model_class.model_name.human) %>

<p>
  <strong><%= model_class.human_attribute_name(:name) %>:</strong><br>
  <%= @diagnosed_object.name %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:state) %>:</strong><br>
  <%= @diagnosed_object.state %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:directive) %>:</strong><br>
  <%= @diagnosed_object.directive %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:expert_system_id) %>:</strong><br>
  <%= @diagnosed_object.expert_system_id %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:prefer_beef) %>:</strong><br>
  <%= @diagnosed_object.prefer_beef %>
</p>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              expert_system_diagnosed_objects_path(@expert_system), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_expert_system_diagnosed_object_path(@expert_system), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              [@expert_system, @diagnosed_object],
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
